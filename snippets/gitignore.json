{
    "Gitignore Template": {
        "prefix": "dgitignore",
        "body": [
            "# Node.js and NPM",
            "node_modules/",
            "/.pnp",
            ".pnp.js",
            "",
            "# Compiled output",
            "/dist/",
            "/build/",
            "/out/",
            "/public/bundle.js",
            "",
            "# Environment files",
            ".env",
            ".env.local",
            ".env.development.local",
            ".env.test.local",
            ".env.production.local",
            "",
            "# MacOS files",
            ".DS_Store",
            "ios/",
            "",
            "# Cache files",
            "/.eslintcache",
            "/.yarn",
            "/.yarn-integrity",
            "/.cache/",
            "",
            "# Log files",
            "logs/",
            "*.log",
            "npm-debug.log*",
            "yarn-debug.log*",
            "yarn-error.log*",
            "pnpm-debug.log*",
            "",
            "# Editor-specific files",
            ".vscode/",
            ".idea/",
            "*.suo",
            "*.ntvs*",
            "*.njsproj",
            "*.sln",
            "",
            "# Dependency directories",
            "/bower_components/",
            "/jspm_packages/",
            "",
            "# Typescript configuration",
            "tsconfig.json",
            "tsconfig.tsbuildinfo",
            "",
            "# Grunt intermediate storage (https://gruntjs.com/creating-plugins#storing-task-files)",
            ".grunt",
            "",
            "# Android",
            "android/",
            "",
            "# Jest",
            "coverage/"
        ],
        "description": "Make a gitignore for most projects"
    }
}